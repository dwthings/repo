<?xml version="1.0" encoding="utf-8"?>
<window>
	<defaultcontrol always="true">302</defaultcontrol>
	<coordinates>
		<left>200</left>
		<top>50</top>
	</coordinates>
	<animation effect="fade" start="0, 100" time="100" tween="quadratic" >WindowOpen</animation>
	<animation effect="fade" time="100">WindowClose</animation>
	<controls>
		<control type="image">
			<description>Image</description>
			<width>900</width>
			<height>590</height>
			<texture>background5.png</texture>
			<colordiffuse></colordiffuse>
			<bordertexture border="2" colordiffuse="black">white.png</bordertexture>
			<!-- <bordersize>5</bordersize> -->
		</control>
		<control type="button" id="302">
			<left>750</left>
			<top>540</top>
			<description>Close</description>
			<width>100</width>
			<height>40</height>
			<texturefocus colordiffuse="deepskyblue">close.png</texturefocus>
            <texturenofocus colordiffuse="grey">close.png</texturenofocus>
			<bordersize>5</bordersize>
			<onleft>301</onleft>
            <onright>301</onright>
		</control>
		<control type="textbox" id = "300">
			<description>text</description>
			<top>15</top>
			<left>50</left>
			<width>880</width>
			<height>490</height>
			<font>font13</font>
			<align>left</align>
			<!-- <aligny>center</aligny> -->
			<textcolor>white</textcolor>
			<shadowcolor>black</shadowcolor>
			<pagecontrol>301</pagecontrol>
		</control>
		<control type="scrollbar" id="301">
			<description>My first scroll bar control</description>
			<left>885</left>
			<top>10</top>
			<width>10</width>
			<height>570</height>
			<visible>true</visible>
			<texturesliderbackground></texturesliderbackground>
			<texturesliderbar>white.png</texturesliderbar>
			<texturesliderbarfocus>white.png</texturesliderbarfocus>
			<textureslidernib></textureslidernib>
			<textureslidernibfocus></textureslidernibfocus>
			<pulseonselect></pulseonselect>
			<orientation>vertical</orientation>
			<showonepage>true</showonepage>
			<onup></onup>
			<ondown></ondown>
			<onleft>302</onleft>
			<onright>302</onright>
		</control>
	</controls>
</window>