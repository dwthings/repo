<?xml version="1.0" encoding="UTF-8"?>
<window>
	<defaultcontrol>9000</defaultcontrol>
	<onload condition="Control.IsVisible(10)">SetFocus(10)</onload>
	<coordinates>
		<system>1</system>
		<top>260</top>
		<left>504</left>
	</coordinates>
	<include>DialogOpenCloseAnimation</include>
	<controls>
		<control type="label" id="1">
			<description>fake heading label</description>
			<width>0</width>
			<height>0</height>
			<font />
			<visible>false</visible>
		</control>
		<control type="image">
			<top>56</top>
			<left>25</left>
			<width>898</width>
			<height>449</height>
			<texture border="40">keyboard_back.png</texture>
			<visible>[String.IsEmpty(Skin.String(dialogpopup))]</visible>
			<visible>!Skin.HasSetting(Disable.Dialog.Popup)</visible>
		</control>
		<control type="image">
			<top>56</top>
			<left>25</left>
			<width>898</width>
			<height>449</height>
			<texture colordiffuse="$VAR[DialogBoxColor]" fallback="keyboard_back.png" border="40">$INFO[Skin.String(dialogpopup)]</texture>
			<visible>!Skin.HasSetting(Disable.Dialog.Popup)</visible>
		</control>
		<control type="image">
			<left>46</left>
			<top>78</top>
			<width>856</width>
			<height>40</height>
			<texture border="5">dialogs/default/inner.png</texture>
		</control>
		<control type="image">
			<left>46</left>
			<top>130</top>
			<width>856</width>
			<height>280</height>
			<texture border="5">dialogs/default/inner.png</texture>
		</control>		
		<control type="progress">
			<description>Progressbar</description>
			<left>46</left>
			<top>78</top>
			<width>856</width>
			<height>35</height>
			<info>System.Progressbar</info>
			<colordiffuse>$VAR[DialogBoxProgressBar]</colordiffuse>
			<visible>Window.IsActive(progressdialog)</visible>
		</control>
		<control type="label">
			<description>header label</description>
			<left>46</left>
			<top>78</top>
			<width>856</width>
			<height>40</height>
			<textoffsetx>15</textoffsetx>
			<font>font15_title</font>
			<label>$INFO[System.Progressbar, ,%]</label>
			<align>center</align>
			<textcolor>white</textcolor>
		</control>
		<control type="textbox" id="9">
			<description>dialog line 1/2/3</description>
			<left>55</left>
			<top>130</top>
			<width>825</width>
			<height>280</height>
			<font>font15_textbox</font>
			<label />
			<textcolor>$VAR[DialogBoxContentTextColor]</textcolor>
			<autoscroll time="3000" delay="4000" repeat="5000">true</autoscroll>
		</control>
		<control type="grouplist" id="9000">
			<left>45</left>
			<top>427</top>
			<width>856</width>
			<orientation>horizontal</orientation>
			<align>center</align>
			<itemgap>14</itemgap>
			<control type="button" id="11">
				<description>Yes button</description>
				<width>250</width>
				<height>60</height>
				<label>107</label>
				<font>font15_title</font>
				<textcolor>$VAR[DialogBoxButtonTextUnselectedColor]</textcolor>
				<focusedcolor>$VAR[DialogBoxButtonTextSelectedColor]</focusedcolor>
				<align>center</align>
				<onleft>10</onleft>
				<onright>10</onright>
				<onup>11</onup>
				<ondown>11</ondown>
				<texturefocus colordiffuse="$VAR[DialogBoxButtonSelectedColor]" border="6">thumbs/panel_borderfo.png</texturefocus>
				<texturenofocus colordiffuse="$VAR[DialogBoxButtonUnselectedColor]" border="6">thumbs/panel_bordernf.png</texturenofocus>
			</control>
			<control type="button" id="10">
				<description>No button</description>
				<left>480</left>
				<top>427</top>
				<width>250</width>
				<height>60</height>
				<label>106</label>
				<font>font15_title</font>
				<textcolor>$VAR[DialogBoxButtonTextUnselectedColor]</textcolor>
				<focusedcolor>$VAR[DialogBoxButtonTextSelectedColor]</focusedcolor>
				<align>center</align>
				<onleft>11</onleft>
				<onright>11</onright>
				<onup>10</onup>
				<ondown>10</ondown>
				<texturefocus colordiffuse="$VAR[DialogBoxButtonSelectedColor]" border="6">thumbs/panel_borderfo.png</texturefocus>
				<texturenofocus colordiffuse="$VAR[DialogBoxButtonUnselectedColor]" border="6">thumbs/panel_bordernf.png</texturenofocus>
			</control>
		</control>
	</controls>
</window>