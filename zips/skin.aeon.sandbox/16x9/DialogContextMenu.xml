			<width>400</width>
			<height min="60" max="1080">auto</height>
		</control>
		<control type="button" id="1000">
		    <width>360</width>
			<height>55</height>
			<font>font14</font>
			<textcolor>$VAR[ContextMenuTextUnselectedColor]</textcolor>
